#A __set local c = ofCanvas(this) \; local args = c:getArgs() \; local
xpos \, ypos \, size = args[1] \, args[2] \, args[3] \; local value
= 1 \; local clock = ofClock(this \, "delay") \; \; function M.new()
\; ofWindow.addListener("mousePressed" \, this) \; end \; \; function
M.value(bang) \; clock:delay(200) \; value = 0 \; return bang \; end
\; \; function M.bang() \; ofPushMatrix() \; ofPushStyle() \; ofSetRectMode(OF_RECTMODE_CORNER)
\; ofFill() \; ofSetColor(255) \; ofDrawRectangle(xpos \, ypos \, size
\, size) \; ofSetLineWidth(2) \; if value == 0 then \; ofSetColor(0)
\; ofSetCircleResolution(100) \; ofDrawCircle(xpos + size / 2 \, ypos
+ size / 2 \, size / 2.5) \; end \; ofNoFill() \; ofSetColor(0) \;
ofSetCircleResolution(100) \; ofDrawCircle(xpos + size / 2 \, ypos
+ size / 2 \, size / 2.5) \; ofDrawRectangle(xpos \, ypos \, size \,
size) \; ofPopStyle() \; ofPopMatrix() \; end \; \; function M.delay()
\; value = 1 \; end \; \; function M.free() \; ofWindow.removeListener("mousePressed"
\, this) \; end \; \; function M.mousePressed(e) \; if ofGetMouseX()
> xpos and ofGetMouseX() < xpos + size and ofGetMouseY() > ypos and
ofGetMouseY() < ypos + size then \; clock:delay(200) \; value = 0 \;
return bang \; end \; end \;;
